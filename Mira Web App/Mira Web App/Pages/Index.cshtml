@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" type="text/css" href="StyleSheet.css">
    <style>
        body {
            background: black;
        }

        #navigationbar a {
            position: absolute;
            left: -225px;
            transition: 0.5s;
            padding: 15px;
            width: 230px;
            color: white;
            border-radius: 0 5px 5px 0;
        }

        #widget {
            top: 55px;
            background-color: white;
        }

        #navigationbar a:hover {
            left: 0;
        }


        #div1, #div2, #div3, #div4, #div5, #div6, #div7, #div8, #div9, #div10, #div11, #div12, #div13, #div14, #div15, #div16, #div17, #div18, #div19, #div20, #div21, #div22, #div23, #div23,
        #div24, #div25, #div26, #div27, #div28, #div29, #div30, #div31, #div32, #div33, #div34, #div35 {
            position: relative;
            height: 150px;
            width: 150px;
            border: 1px solid white;
            float: left;
            top: 5px;
        }

        #clockcontainer {
            position: relative;
            height: 150px;
            width: 300px;
            cursor: pointer;
            border: 1px solid white;
            float: left;
            top: 5px;
            z-index: 3;
        }

        #youtubecontainer {
            position: relative;
            height: 150px;
            width: 300px;
            cursor: pointer;
            border: 1px solid white;
            float: left;
            top: 5px;
            z-index: 3;
        }

        #calendarcontainer {
            position: relative;
            height: 150px;
            width: 300px;
            cursor: pointer;
            border: 1px solid white;
            float: left;
            top: 5px;
            z-index: 3;
        }

        #clock {
            z-index: -1;
            position: relative;
            left: 40px;
        }

        #youtube {
            position: relative;
            left: 30px;
        }

        #calendar {
            position: relative;
            left: 100px;
        }

        p {
            color: white;
            font-size: 20px;
        }

        img {
            cursor: pointer;
        }

        input {
            background-color: black;
            color: white;
        }
    </style>
</head>

<body>

    <div id="navigationbar">
        <a id="widget">

            <ul>
                <li><input type="button" value="Reset" onClick="location.href=location.href"></li>
                <li> <img id="drag1" src="https://source.unsplash.com/random/150x150" draggable="true" ondragstart="drag(event)" width="150" height="150"></li>
                <li> <img id="drag5" src="https://picsum.photos/200/300/?random" draggable="true" ondragstart="drag(event)" width="150" height="150"></li>
                <li>
                    <div id="clockcontainer" ondrop="drop(event)" ondragover="allowDrop(event)" draggable="true" ondragstart="drag(event)">
                        <iframe id="clock" src="https://www.zeitverschiebung.net/clock-widget-iframe-v2?language=en&size=medium&timezone=America%2FNew_York" scrolling='no' height=200 width=250 frameborder="0" seamless></iframe>
                    </div>
                </li>
                <li>
                    <div id="youtubecontainer" ondrop="drop(event)" ondragover="allowDrop(event)" draggable="true" ondragstart="drag(event)">
                        <iframe id="youtube" src="https://www.youtube.com/embed/UO5CX_oNfXg" width="250" height="200" frameborder="0" draggable="true"></iframe>
                    </div>
                </li>
                <li>
                    <div id="calendarcontainer" ondrop="drop(event)" ondragover="allowDrop(event)" draggable="true" ondragstart="drag(event)">
                        <iframe id="calendar" src='http://cdn.instantcal.com/cvir.html?id=cv_nav1&theme=RE&ntype=cv_datepicker&file=http%3A%2F%2Fwww.instantcal.com%2Ftest.ics&border_radius=5' ondragstart="drag(event)" allowTransparency='true' scrolling='no' frameborder=0 height=200 width=200></iframe>
                    </div>
                </li>
            </ul>
        </a>

